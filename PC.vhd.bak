library ieee;
use ieee.std_logic_1164.all;

Entity mux is
 Port (
  pc : in std_logic_vector(15 downto 0);
  pc_write : in std_logic;
  pc_out : out std_logic_vector(15 downto 0));
 End;

Architecture behavior of mux is
  begin
   process(pc, pc_write)
   begin
    if(pc_write = '1') then
      pc_out <= pc;
   end process;
  end;